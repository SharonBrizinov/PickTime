{
  "name": "PickTime",
  "description": "extract tokens from myvisit.com to populate picktime",
  "version": "0.1",
  "permissions": [
      "storage"
    ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://myvisit.com/*"
      ],
      "js": ["content.js"]
    }
  ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "PickTime"
    },
    "manifest_version": 2
}